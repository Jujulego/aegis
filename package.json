{
  "name": "@jujulego/aegis",
  "version": "2.1.1",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@google.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/aegis"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "vitest run",
    "test:types": "vitest typecheck"
  },
  "dependencies": {
    "@jujulego/event-tree": "^4.3.0",
    "@jujulego/utils": "^2.0.0",
    "immer": "^10.0.3"
  },
  "devDependencies": {
    "@jujulego/jill": "2.3.5",
    "@jujulego/vite-plugin-swc": "1.1.1",
    "@microsoft/eslint-formatter-sarif": "3.0.0",
    "@swc/cli": "0.3.3",
    "@swc/core": "1.3.106",
    "@types/node": "18.19.10",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitest/coverage-v8": "0.34.6",
    "eslint": "8.56.0",
    "eslint-plugin-vitest": "0.3.20",
    "jsdom": "22.1.0",
    "shx": "0.3.4",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "0.34.6"
  },
  "packageManager": "yarn@4.0.2",
  "dependenciesMeta": {
    "vitest@0.34.6": {
      "unplugged": true
    }
  }
}
