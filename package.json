{
  "name": "@jujulego/aegis",
  "version": "2.0.0-alpha.6",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@google.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/aegis"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "shx rm -r dist",
    "build": "jill group 'build:esm // build:types'",
    "build:esm": "swc -d dist src",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "jest",
    "publish": "yarn npm publish --access public --tolerate-republish"
  },
  "dependencies": {
    "@jujulego/event-tree": "^2.7.0",
    "@jujulego/utils": "^1.4.2"
  },
  "devDependencies": {
    "@jujulego/jill": "2.3.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.78",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.0",
    "@types/node": "16.18.41",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.47.0",
    "eslint-plugin-vitest": "0.2.8",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "shx": "0.3.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.2"
}
